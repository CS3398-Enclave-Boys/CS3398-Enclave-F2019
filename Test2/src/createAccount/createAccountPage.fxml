                  <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="204.0" layoutY="118.0" prefHeight="46.0" prefWidth="166.0" text="User Name:" textAlignment="RIGHT">
                     <font>
                        <Font size="31.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER_RIGHT" layoutX="229.0" layoutY="185.0" prefHeight="46.0" prefWidth="141.0" text="Password:">
                     <font>
                        <Font size="31.0" />
                     </font>
                  </Label>
                  <PasswordField fx:id="passwordField" layoutX="400.0" layoutY="196.0" prefHeight="25.0" prefWidth="245.0" />
                  <TextField fx:id="userNameField" layoutX="400.0" layoutY="121.0" prefHeight="25.0" prefWidth="245.0" />
                  <Button fx:id="createAccountButton" layoutX="645.0" layoutY="479.0" mnemonicParsing="false" text="Create Account" textAlignment="CENTER" textFill="#0a0808">
                     <effect>
                        <Glow level="0.33" />
                     </effect>
                  </Button>
                  <PasswordField fx:id="confirmPasswordField" layoutX="400.0" layoutY="267.0" prefHeight="25.0" prefWidth="245.0" />
