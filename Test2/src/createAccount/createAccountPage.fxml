                  <Label contentDisplay="CENTER" layoutX="326.0" prefHeight="53.0" prefWidth="249.0" text="Create Account" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <Button fx:id="backButton" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#createAccountBack" text="Back" />
